{
  "name": "seo-nlp-app",
  "version": "1.0.0",
  "description": "Cost-optimized SEO NLP application with intelligent auto-scaling and tiered AI models",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "echo 'No build step required for Node.js app'",
    "deploy": "./scripts/deploy.sh",
    "docker:build": "docker build -t seo-nlp-app .",
    "docker:run": "docker run -p 3000:3000 seo-nlp-app",
    "health-check": "curl -f http://localhost:3000/health || exit 1"
  },
  "keywords": [
    "seo",
    "nlp",
    "ai",
    "bedrock",
    "aws",
    "fargate",
    "cost-optimization",
    "auto-scaling"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
    "@aws-sdk/client-cloudwatch": "^3.450.0",
    "@aws-sdk/client-dynamodb": "^3.859.0",
    "@aws-sdk/client-ecs": "^3.450.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.450.0",
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/lib-dynamodb": "^3.859.0",
    "aws-sdk": "^2.1490.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.10.0",
    "multer": "^2.0.2",
    "pdf-parse": "^1.1.1",
    "redis": "^4.6.10",
    "stripe": "^18.4.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/seo-nlp-app.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/seo-nlp-app/issues"
  },
  "homepage": "https://github.com/yourusername/seo-nlp-app#readme",
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
