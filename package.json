{
  "name": "seo-nlp-bedrock-app",
  "version": "1.0.0",
  "description": "SEO-friendly NLP application with AWS Bedrock integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev-clean": "pkill -f 'node server.js' || true && sleep 2 && nodemon server.js",
    "kill-port": "lsof -ti:3000 | xargs -r kill -9",
    "clean-start": "npm run kill-port && sleep 2 && npm run dev",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1490.0",
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "stripe": "^14.7.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "mammoth": "^1.6.0",
    "pdf-parse": "^1.1.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": ["seo", "nlp", "bedrock", "aws", "subscription"],
  "author": "Your Name",
  "license": "MIT"
}
